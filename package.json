  "version": "2.0.0",
  "description": "HomePage of BTB Lab. for React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "cleanModules": "rimraf node_modules",
    "updateHomePage": "npm run build-prod && gulp updateHomePage",
    "backupCodebase": "gulp backupCodebase",
    "build-prod": "webpack --progress --color --config webpack.prod.js",
    "build-dev": "webpack --progress --color --config webpack.dev.js",
    "start": "npm run start-dev",
    "start-prod": "webpack-dev-server --open --devtool eval --progress --color --content-base build --config webpack.prod.js",
    "start-dev": "webpack-dev-server --open --devtool eval --progress --color --content-base build --config webpack.dev.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint --ext .js,.jsx .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
